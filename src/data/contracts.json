{
  "contracts": [
    {
      "name": "Ōkinoko Escrow",
      "id": 1,
      "vscId": "vsc1BgfucQVHwYBHuK2yMEv4AhYua9rtQ45Uoe",
      "owner": "tibfox.vsc",
      "deployedOn": "2025-10-18T22:02:33",
      "verifiedOn": "2025-10-20T08:37:05",
      "description": "This contract acts as an on-chain escrow between two parties, optionally including an arbitrator for dispute resolution. Deposit funds securely and choose whether to release them or request a refund.",
      "functions": [
        {
          "name": "e_create",
          "friendlyName": "Create Escrow",
          "description": "Set up a new escrow by choosing who will receive the funds, how much to send, and who will act as arbitrator.\n! Don’t forget: You’ll get an escrow ID after creation. You’ll need it later to manage the escrow.",
          "parse":"csv",
          "delimiter":"|",
          "excludeKeys":true,
          "parameters": [
            {
              "name": "Descriptive Name",
              "payloadName": "name",
              "type": "string",
              "mandatory": true
            },
            {
              "name": "Receiver of the Amount",
              "payloadName": "to",
              "type": "address",
              "mandatory": true
            },
            {
              "name": "amount / asset",
              "payloadName": "vscIntent",
              "type": "vscIntent",
              "mandatory": true
            },
            {
              "name": "Arbitrator (in case of Disputes)",
              "payloadName": "arb",
              "type": "address",
              "mandatory": true
            }
          ]
        },
        {
          "name": "e_decide",
          "friendlyName": "Add Decision",
          "description": "If you’re part of the escrow, you can send in your decision. Don’t worry: you can change it anytime before the escrow is finalized.",
          "parse":"csv",
          "delimiter":"|",
          "excludeKeys":true,
          "parameters": [
            {
              "name": "Escrow Id",
              "payloadName": "id",
              "type": "number",
              "asString": false,
              "mandatory": true
            },
            {
              "name": "Release Funds to the Receiver?",
              "payloadName": "d",
              "type": "bool",
              "boolFalse":"f",
              "boolTrue":"r",
              "asString": true,
              "mandatory": false
            }
          ]
        },
        {
          "name": "e_get",
          "friendlyName": "Get Escrow",
          "description": "This function is used to get all data related to a give escrow ID.",
          "parse": "raw",
          "parameters": [
            {
              "name": "Escrow Id",
              "payloadName": "id",
              "type": "number",
              "asString": false,
              "mandatory": true
            }
          ]
        }
      ]
    },
